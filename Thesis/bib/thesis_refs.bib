@article{ban2005laser,
	author = {H. Y. Ban and M. Jacka and J. L. Hanssen and J. Reader and J. J. McClelland},
	journal = {Opt. Express},
	keywords = {Oscillator strengths; Laser trapping; Laser arrays; Laser sources; Optical clocks; Optical cooling; Quantum information processing; Tunable lasers},
	number = {8},
	pages = {3185--3195},
	publisher = {OSA},
	title = {Laser cooling transitions in atomic erbium},
	volume = {13},
	month = {04},
	year = {2005},
	doi = {10.1364/OPEX.13.003185},%url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-13-8-3185},
	abstract = {Abstract We discuss laser cooling opportunities in atomic erbium, identifying five J{\textrightarrow}J$+$1 transitions from the 4f126s2 3H6 ground state that are accessible to common visible and near-infrared continuous-wave tunable lasers. We present lifetime measurements for the 4f11(4Io15/2)5d5/26s2 (15/2, 5/2)7 o state at 11888 cm{\textminus}1 and the 4f11(4Io 13/2)5d3/26s2 (13/2, 5/2)7o state at 15847 cm{\textminus}1, showing values of 20{\textpm}4 {\textmu}s and 5.6{\textpm}1.4 {\textmu}s, respectively. We also present a calculated value of 13{\textpm}7 s{\textminus}1 for the transition rate from the 4f11(4Io 15/2)5d3/26s2 (15/2, 3/2)7 o state at 7697 cm-1 to the ground state, based on scaled Hartree-Fock energy parameters. Laser cooling on these transitions in combination with a strong, fast (5.8 ns) laser cooling transition at 401 nm, suggest new opportunities for narrowband laser cooling of a large-magnetic moment atom, with possible applications in quantum information processing, high-precision atomic clocks, quantum degenerate gases, and deterministic single-atom doping of materials.},
}




